{
  "expo": {
    "name": "Fisiko",
    "slug": "fisiko",
    "scheme": "fisiko",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#FF6B35"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fisiko.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FF6B35"
      },
      "package": "com.fisiko.app"
    },
    "web": {
      "favicon": ""
    },
    "plugins": [
      "expo-camera",
      "expo-image-picker",
      "expo-notifications",
      "expo-web-browser",
      "expo-apple-authentication"
    ]
  }
}
